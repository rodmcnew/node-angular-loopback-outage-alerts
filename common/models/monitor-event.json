{
  "name": "MonitorEvent",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "date": {
      "type": "date",
      "required": true
    },
    "type": {
      "type": "string",
      "required": true,
      "description": "'u' for up, 'd' for down, 's' for started, 'p' for paused"
    },
    "reason": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "monitor": {
      "type": "belongsTo",
      "model": "Monitor",
      "foreignKey": "monitorId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
