{
  "name": "MonitorPing",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "date": {
      "type": "date",
      "required": true
    },
    "up": {
      "type": "boolean",
      "required": true
    },
    "latency": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "monitor": {
      "type": "belongsTo",
      "model": "Monitor",
      "foreignKey": "monitorId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
